    <script type="text/javascript">

        //checklogin - logout
        if (typeof sessionStorage.token_value == "undefined" || sessionStorage.token_value == "") {
            window.location.href = "/User/Login";
        }
        else {
            //check role id -- Bao 22.5.2017
            if (sessionStorage.roleid != 2)
            {
                document.getElementById("menu").children[2].style.display = "none" //[2] Xem thong tin Agent
            } else if (sessionStorage.roleid != 1)
            {
                document.getElementById("menu").children[3].style.display = "none" //[3]Doi mat khau 
            }
            //
            $.ajax({
                type: "POST",
                data: JSON.stringify({ token: sessionStorage.token_value }),
                url: "http://localhost:8080/RFJVService/rest/user/csl"
            }).done(function (data) {

                if (data == "0") {
                    window.location.href = "/User/Login";
                }
                else {
                    var fullname = data.fullname;
                    $("#lblWelcome").text("Welcome " + fullname);
                }

            }).fail(function (xhr, status, error) {
                console.log(xhr);
                console.log(status);
                console.log(error);
            });
           
        }

        $("#btnLogout").click(function () {
            $.ajax({
                type: "POST",
                data: JSON.stringify({ token: sessionStorage.token_value }),
                url: "http://localhost:8080/RFJVService/rest/user/checklogout"
            }).done(function (data) {
                window.location.href = "/User/Login";
            }).fail(function (xhr, status, error) {
                console.log(xhr);
                console.log(status);
                console.log(error);
            });
        });

        // This function is called from the pop-up menus to transfer to
        // a different page. Ignore if the value returned is a null string:
        function goPage(newURL) {

            // if url is empty, skip the menu dividers and reset the menu selection to default
            if (newURL != "") {

                // if url is "-", it is this page -- reset the menu:
                if (newURL == "-") {
                    resetMenu();
                }
                    // else, send page to designated URL
                else {
                    document.location.href = newURL;
                }
            }
        }

        // resets the menu selection upon entry to this page:
        function resetMenu() {
            document.gomenu.selector.selectedIndex = 2;
        }
    </script>
